{
    "id": "org.kde.kmymoney",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.9",
    "sdk": "org.kde.Sdk",
    "command": "kmymoney",
    "desktop-file-name-suffix": " (Nightly)",
    "rename-icon": "kmymoney",
    "copy-icon": true,
    "finish-args": [
        "--device=dri",
        "--share=ipc",
        "--share=network",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--system-talk-name=org.freedesktop.UDisks2"
    ],
    "cleanup": [
        "*.a",
        "*.la",
        "/include",
        "/lib/cmake",
        "/lib/pkgconfig",
        "/mkspecs",
        "/share/aclocal",
        "/share/doc",
        "/share/info",
        "/share/man"
    ],
    "modules": [
        {
            "name": "qgpgme",
            "buildsystem": "autotools",
            "config-opts": [
                "--enable-languages=cpp,qt6",
                "--disable-glibtest",
                "--disable-gpgconf-test",
                "--disable-gpg-test",
                "--disable-gpgsm-test",
                "--disable-g13-test"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gnupg.org/ftp/gcrypt/gpgme/gpgme-1.24.3.tar.bz2",
                    "sha256": "bfc17f5bd1b178c8649fdd918956d277080f33df006a2dc40acdecdce68c50dd",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 1239,
                        "stable-only": true,
                        "url-template": "https://gnupg.org/ftp/gcrypt/gpgme/gpgme-$version.tar.bz2",
                        "versions": {
                            "<": "2.0.0"
                        }
                    }
                }
            ],
            "cleanup": [
                "/bin"
            ]
        },
        {
            "name": "qtkeychain",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_WITH_QT6=ON",
                "-DBUILD_TESTING=OFF"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/frankosterfeld/qtkeychain/archive/refs/tags/0.15.0.tar.gz",
                    "sha256": "f4254dc8f0933b06d90672d683eab08ef770acd8336e44dfa030ce041dc2ca22",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 4138,
                        "stable-only": true,
                        "url-template": "https://github.com/frankosterfeld/qtkeychain/archive/refs/tags/$version.tar.gz"
                    }
                }
            ]
        },
        {
            "name": "alkimia",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DQT_MAJOR_VERSION=6",
                "-DBUILD_APPLETS=OFF",
                "-DBUILD_TESTING=OFF"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/office/alkimia.git"
                }
            ]
        },
        {
            "name": "kdiagram",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-DBUILD_TESTING=OFF"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/graphics/kdiagram.git"
                }
            ]
        },
        {
            "name": "libofx",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/libofx/libofx/releases/download/0.10.9/libofx-0.10.9.tar.gz",
                    "sha256": "1ca89ff7d681c9edad312172ac464231a8de686e653213612f9417492cef0d37",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 7309,
                        "stable-only": true,
                        "url-template": "https://github.com/libofx/libofx/releases/download/0.10.9/libofx-$version.tar.gz"
                    }
                }
            ],
            "cleanup": [
                "/bin"
            ],
            "modules": [
                {
                    "name": "opensp",
                    "buildsystem": "autotools",
                    "rm-configure": true,
                    "config-opts": [
                        "--disable-doc-build",
                        "--disable-static"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://sourceforge.net/projects/openjade/files/opensp/1.5.2/OpenSP-1.5.2.tar.gz",
                            "sha256": "57f4898498a368918b0d49c826aa434bb5b703d2c3b169beb348016ab25617ce",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 377818,
                                "stable-only": true,
                                "url-template": "https://sourceforge.net/projects/openjade/files/opensp/$version/OpenSP-$version.tar.gz"
                            }
                        },
                        {
                            "type": "shell",
                            "commands": [
                                "cp -p /usr/share/automake-*/config.{sub,guess} ."
                            ]
                        },
                        {
                            "type": "script",
                            "dest-filename": "autogen.sh",
                            "commands": [
                                "autoreconf -vfi"
                            ]
                        }
                    ],
                    "cleanup": [
                        "/bin"
                    ]
                },
                {
                    "name": "libxml++",
                    "buildsystem": "meson",
                    "config-opts": [
                        "-Dbuild-examples=false",
                        "-Dbuild-tests=false"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://download.gnome.org/sources/libxml++/2.42/libxml++-2.42.3.tar.xz",
                            "sha256": "74b95302e24dbebc56e97048e86ad0a4121fc82a43e58d381fbe1d380e8eba04",
                            "x-checker-data": {
                                "type": "gnome",
                                "name": "libxml++",
                                "versions": {
                                    "<": "2.91"
                                }
                            }
                        }
                    ],
                    "cleanup": [
                        "/bin"
                    ],
                    "modules": [
                        {
                            "name": "glibmm",
                            "buildsystem": "meson",
                            "config-opts": [
                                "-Dbuild-examples=false"
                            ],
                            "sources": [
                                {
                                    "type": "archive",
                                    "url": "https://download.gnome.org/sources/glibmm/2.66/glibmm-2.66.8.tar.xz",
                                    "sha256": "64f11d3b95a24e2a8d4166ecff518730f79ecc27222ef41faf7c7e0340fc9329",
                                    "x-checker-data": {
                                        "type": "gnome",
                                        "name": "glibmm",
                                        "versions": {
                                            "<": "2.68"
                                        }
                                    }
                                }
                            ],
                            "modules": [
                                {
                                    "name": "sigc++",
                                    "buildsystem": "meson",
                                    "config-opts": [
                                        "-Dbuild-examples=false",
                                        "-Dbuild-tests=false"
                                    ],
                                    "sources": [
                                        {
                                            "type": "archive",
                                            "url": "https://download.gnome.org/sources/libsigc++/2.12/libsigc++-2.12.1.tar.xz",
                                            "sha256": "a9dbee323351d109b7aee074a9cb89ca3e7bcf8ad8edef1851f4cf359bd50843",
                                            "x-checker-data": {
                                                "type": "gnome",
                                                "name": "libsigc++",
                                                "versions": {
                                                    "<": "2.99"
                                                }
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "name": "aqbanking",
            "buildsystem": "autotools",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/aqbanking/aqbanking/archive/refs/tags/6.6.1.tar.gz",
                    "sha256": "72ff6d41743b122bb5d4edf7d9f54c66126b7392f1185f7cf50330c9e308e85e",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 6890,
                        "stable-only": true,
                        "url-template": "https://github.com/aqbanking/aqbanking/archive/refs/tags/$version.tar.gz"
                    }
                },
                {
                    "type": "shell",
                    "commands": [
                        "make -fMakefile.cvs",
                        "./configure",
                        "make typedefs",
                        "make typefiles"
                    ]
                }
            ],
            "cleanup": [
                "/bin"
            ],
            "modules": [
                {
                    "name": "xmlsec1",
                    "buildsystem": "autotools",
                    "config-opts": [
                        "--disable-static"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://www.aleksey.com/xmlsec/download/xmlsec1-1.3.7.tar.gz",
                            "sha256": "d82e93b69b8aa205a616b62917a269322bf63a3eaafb3775014e61752b2013ea",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 5200,
                                "stable-only": true,
                                "url-template": "https://www.aleksey.com/xmlsec/download/xmlsec1-$version.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/bin"
                    ]
                },
                {
                    "name": "gwenhywfar",
                    "buildsystem": "autotools",
                    "config-opts": [
                        "--with-guis=qt6"
                    ],
                    "build-options": {
                        "env": {
                            "GPGRT_CONFIG": "/app/bin/gpgrt-config"
                        }
                    },
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://github.com/aqbanking/gwenhywfar/archive/refs/tags/5.12.2.tar.gz",
                            "sha256": "64e044f41a4ea59d3c5a7afe77ed8fed3ee850277f9310183bb5fcb0b2329633",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 6891,
                                "stable-only": true,
                                "url-template": "https://github.com/aqbanking/gwenhywfar/archive/refs/tags/$version.tar.gz"
                            }
                        },
                        {
                            "type": "shell",
                            "commands": [
                                "make -fMakefile.cvs"
                            ]
                        }
                    ],
                    "cleanup": [
                        "/bin"
                    ],
                    "modules": [
                        {
                            "name": "libgcrypt",
                            "buildsystem": "autotools",
                            "config-opts": [
                                "--disable-static"
                            ],
                            "sources": [
                                {
                                    "type": "archive",
                                    "url": "https://www.gnupg.org/ftp/gcrypt/libgcrypt/libgcrypt-1.11.2.tar.bz2",
                                    "sha256": "6ba59dd192270e8c1d22ddb41a07d95dcdbc1f0fb02d03c4b54b235814330aac",
                                    "x-checker-data": {
                                        "type": "anitya",
                                        "project-id": 1623,
                                        "stable-only": true,
                                        "url-template": "https://www.gnupg.org/ftp/gcrypt/libgcrypt/libgcrypt-$version.tar.bz2"
                                    }
                                }
                            ],
                            "cleanup": [
                                "/bin"
                            ],
                            "modules": [
                                {
                                    "name": "libgpg-error",
                                    "buildsystem": "autotools",
                                    "config-opts": [
                                        "--disable-static"
                                    ],
                                    "sources": [
                                        {
                                            "type": "archive",
                                            "url": "https://www.gnupg.org/ftp/gcrypt/libgpg-error/libgpg-error-1.55.tar.bz2",
                                            "sha256": "95b178148863f07d45df0cea67e880a79b9ef71f5d230baddc0071128516ef78",
                                            "x-checker-data": {
                                                "type": "anitya",
                                                "project-id": 1628,
                                                "stable-only": true,
                                                "url-template": "https://www.gnupg.org/ftp/gcrypt/libgpg-error/libgpg-error-$version.tar.bz2"
                                            }
                                        }
                                    ],
                                    "cleanup": [
                                        "/bin",
                                        "/share/common-lisp",
                                        "/share/libgpg-error"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "name": "sqlcipher",
            "buildsystem": "autotools",
            "config-opts": [
                "--disable-tcl",
                "--disable-static",
                "--enable-tempstore=yes"
            ],
            "build-options": {
                "cflags": "-DSQLITE_HAS_CODEC",
                "ldflags": "-lcrypto"
            },
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/sqlcipher/sqlcipher/archive/refs/tags/v4.6.1.tar.gz",
                    "sha256": "d8f9afcbc2f4b55e316ca4ada4425daf3d0b4aab25f45e11a802ae422b9f53a3",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 11213,
                        "stable-only": true,
                        "url-template": "https://github.com/sqlcipher/sqlcipher/archive/refs/tags/v$version.tar.gz",
                        "versions": {
                            "<": "4.7"
                        }
                    }
                }
            ],
            "cleanup": [
                "/bin"
            ]
        },
        {
            "name": "kmymoney",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-DBUILD_WITH_QT6=ON"
            ],
            "builddir": true,
            "sources": [
                {
                    "type": "dir",
                    "path": "."
                }
            ],
            "cleanup": [
                "/share/icons/hicolor/1024x1024"
            ]
        }
    ]
}
